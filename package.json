{
	"name": "sveltekit-remote-debug",
	"description": "This component allows you to debug your remote functions in the style of the sveltekit-superforms library SuperDebug",
	"version": "1.0.7",
	"private": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"dependencies": {
		"svelte": "^5.0.0",
		"@sveltejs/kit": "^2.46.5",
		"shiki": "^3.13.0",
		"lucide-svelte": "^0.545.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.46.5",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/vite": "^4.1.13",
		"lucide-svelte": "^0.545.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"publint": "^0.3.13",
		"shiki": "^3.13.0",
		"svelte": "^5.39.12",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"remote-functions",
		"valibot",
		"validation"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/oliie/remote-debug.git"
	},
	"license": "MIT"
}
